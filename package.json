{
  "name": "arickho-com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "build": "next build",
    "flow": "flow",
    "interactive-upgrade": "npm-upgrade",
    "lint:css": "stylelint './**/*.js'",
    "export": "next export && npm run build",
    "docker:build": "docker build -t arickho-dot-com .",
    "docker:clean": "docker rm -f arickho-dot-com || true",
    "docker:run": "docker run -p 7878:3000 -d --name arickho-dot-com arickho-dot-com && npm run docker:logs",
    "docker:stop": "docker stop arickho-dot-com",
    "docker:start": "docker start arickho-dot-com && npm run docker:logs",
    "docker:logs": "docker logs -f arickho-dot-com",
    "deploy": "npm run docker:build && npm run docker:clean && npm run docker:run"
  },
  "dependencies": {
    "@babel/core": "7.5.5",
    "@redux-beacon/google-analytics-gtag": "1.0.5",
    "gsap": "2.1.3",
    "isomorphic-unfetch": "3.0.0",
    "next-images": "1.1.2",
    "next": "9.0.2",
    "prop-types": "15.6.2",
    "react-dom": "16.8.6",
    "react-redux": "7.0.0",
    "react-reveal": "1.2.2",
    "react": "16.8.6",
    "redux-beacon": "2.1.0",
    "redux-thunk": "2.3.0",
    "redux": "4.0.4",
    "styled-components": "4.2.1",
    "styled-normalize": "8.0.2",
    "webpack": "4.38.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.10.6",
    "express": "4.17.1",
    "npm-upgrade": "2.0.0",
    "redux-devtools-extension": "2.13.5",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "stylelint": "10.1.0"
  }
}
