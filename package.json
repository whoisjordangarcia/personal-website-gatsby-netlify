{
  "name": "jordangarcia.me",
  "private": true,
  "description": "Jordan's personal website",
  "version": "1.0.0",
  "author": "Jordan Garcia <arickho@gmail.com>",
  "repository": "https://github.com/whoisjordangarcia-org/personal-website-gatsby-netlify",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prettier": "prettier \"**/*.{ts,tsx,md,mdx,json,css}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:check": "npm run lint -- --fix-dry-run",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint --syntax css \"./src/**/*.css\"",
    "stylelint:check": "npm run stylelint",
    "stylelint:fix": "npm run stylelint -- --fix",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest"
  },
  "dependencies": {
    "gatsby": "^4.8.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-gtag": "^4.8.0",
    "gatsby-plugin-manifest": "^4.8.0",
    "gatsby-plugin-offline": "^5.8.0",
    "gatsby-plugin-react-helmet": "^5.8.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-typescript": "^4.8.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^6.25.0",
    "gatsby-source-filesystem": "^4.8.0",
    "gatsby-transformer-remark": "^5.8.0",
    "gatsby-transformer-sharp": "^4.25.0",
    "gsap": "^3.2.6",
    "imagemin-pngquant": "^8.0.0",
    "pngquant-bin": "^5.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/gsap": "^1.20.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
